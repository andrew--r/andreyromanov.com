{% macro bookmarkView(bookmark) %}
<section class="bookmark">
  <h3><a href="{{ bookmark.url }}">{{ bookmark.title }}</a></h3>
  <ul aria-label="Tags">
    {% for tag in bookmark.tags %}
      <li class="bookmark__tag text_size_s text_muted">{{ tag }}</li>
    {% endfor %}
  </ul>
</section>
{% endmacro %}
